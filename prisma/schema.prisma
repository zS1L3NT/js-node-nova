generator client {
    provider = "cargo prisma"
    output   = "../src/prisma.rs"
}

datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}

model Config {
    filename  String @id
    shorthand String
    content   String
}

model Secret {
    project String
    path    String
    content String

    @@id([project, path])
}
